# ================================
# Airflow Core Configuration
# ================================
AIRFLOW__CORE__EXECUTOR=LocalExecutor
AIRFLOW__CORE__LOAD_EXAMPLES=False
AIRFLOW__CORE__DAGS_ARE_PAUSED_AT_CREATION=True
AIRFLOW__LOGGING__LOGGING_LEVEL=INFO
AIRFLOW__SCHEDULER__CATCHUP_BY_DEFAULT=False
AIRFLOW__SCHEDULER__DAG_DIR_LIST_INTERVAL=30

# ================================
# Database Connection
# (Replace with your actual credentials)
# ================================
AIRFLOW__DATABASE__SQL_ALCHEMY_CONN=postgresql+psycopg2://airflow:airflow@postgres:5432/airflow

# Leave blank unless overridden
AIRFLOW__CORE__SQL_ALCHEMY_CONN=

# ================================
# Security Keys (Replace before running)
# ================================
AIRFLOW__CORE__FERNET_KEY=REPLACE_ME_FERNET_KEY
AIRFLOW__WEBSERVER__SECRET_KEY=REPLACE_ME_SECRET_KEY

# ================================
# MinIO / S3 Credentials (Dev only)
# ================================
AWS_ACCESS_KEY_ID=REPLACE_ME_ACCESS_KEY
AWS_SECRET_ACCESS_KEY=REPLACE_ME_SECRET_KEY
AWS_DEFAULT_REGION=us-east-1
